class Vehicle
{
    int maxSpeed = 120;
}
class Car extends Vehicle
{
    int maxSpeed = 180;
  
    void display()
    {
        /* print maxSpeed of base class (vehicle) */
        System.out.println("Maximum Speed: " + super.maxSpeed);
    }
}

public class Super_Program
{
    public static void main(String[] args)
    {
        Car ob = new Car();
        ob.display();
    }
}

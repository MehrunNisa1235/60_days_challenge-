public class Hollow_Rectangle
{
   public static void  main(String args[])
   {
	    int n=5;  //row 
	    int m=6;   //col

     for(int i=1;i<=5;i++)
     {
	    for(int j=1;j<=6;j++)
	    {
	       if(i==1 || j==1 || i==n || j==m) 
	       {
		      System.out.print("*");
	       }
	      else
	      {
		      System.out.print(" "); 
	      }
	   } 
		    System.out.println(); 			
    } 
	}
}
